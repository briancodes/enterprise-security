{
  "name": "enterprise-security",
  "version": "1.0.0",
  "description": "Node.js Express implementations of the concepts learned at Angular Academy Enterprise Security course",
  "scripts": {
    "start:m2": "npm-run-all --parallel \"m2:*\"",
    "m2:local": "nodemon module-2/local/local-server.js",
    "m2:remote": "nodemon module-2/remote/remote-server.js",
    "m2:local:debug": "nodemon --inspect module-2/local/local-server.js",
    "m2:remote:debug": "nodemon --inspect module-2/remote/remote-server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/briancodes/enterprise-security.git"
  },
  "author": "briancodes",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet-csp": "^2.9.5"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5"
  }
}
